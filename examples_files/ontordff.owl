    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#exposes -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#exposes">
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Device"/>
